{
    "author": "Steven Olmsted <steven.olmsted@ibigroup.com>",
    "babel": {
        "env": {
            "build": {
                "presets": [
                    "minify"
                ]
            }
        },
        "plugins": [
            "transform-line"
        ],
        "presets": [
            [
                "env",
                {
                    "targets": {
                        "node": "current"
                    }
                }
            ]
        ],
        "sourceMaps": "inline"
    },
    "bugs": "https://github.com/ibi-group/isotropic/issues",
    "contributors": [
        "Duane Sibilly <duane.sibilly@ibigroup.com>"
    ],
    "dependencies": {
        "isotropic-character-fold": "~0.0.0",
        "isotropic-create": "~0.5.0",
        "isotropic-error": "~0.5.0",
        "isotropic-for-in": "~0.5.0",
        "isotropic-initializable": "~0.2.0",
        "isotropic-later": "~0.5.0",
        "isotropic-logger-global": "~0.5.0",
        "isotropic-make": "~0.5.0",
        "isotropic-mixin": "~0.5.0",
        "isotropic-mixin-prototype-chain": "~0.2.0",
        "isotropic-natural-sort": "~0.0.0",
        "isotropic-property-chainer": "~0.5.0",
        "isotropic-prototype-chain": "~0.5.0",
        "isotropic-pubsub": "~0.6.0",
        "isotropic-timeout": "~0.5.0",
        "isotropic-value-to-source": "~0.1.0"
    },
    "description": "A collection of utilities for large JavaScript projects with a consistent API.",
    "devDependencies": {
        "babel-cli": "~6.26.0",
        "babel-istanbul": "~0.12.2",
        "babel-plugin-transform-line": "~0.3.0",
        "babel-preset-env": "~1.6.0",
        "babel-preset-minify": "~0.2.0",
        "chai": "~4.1.2",
        "eslint": "~4.6.1",
        "eslint-plugin-isotropic": "~0.0.0",
        "mocha": "~3.5.1",
        "nsp": "~2.8.0"
    },
    "engines": {
        "node": "^8.4.0",
        "npm": "^5.4.1"
    },
    "eslintConfig": {
        "env": {
            "es6": true
        },
        "extends": "plugin:isotropic/isotropic",
        "parserOptions": {
            "ecmaFeatures": {
                "experimentalObjectRestSpread": true,
                "globalReturn": false,
                "impliedStrict": true,
                "jsx": false
            },
            "ecmaVersion": 2017,
            "sourceType": "module"
        },
        "plugins": [
            "isotropic"
        ],
        "root": true
    },
    "homepage": "https://github.com/ibi-group/isotropic",
    "license": "BSD-3-Clause",
    "name": "isotropic",
    "repository": "ibi-group/isotropic",
    "scripts": {
        "build": "BABEL_ENV=build babel js -d .",
        "lint": "eslint js test",
        "posttest": "[ -z \"$npm_config_coverage\" ] || babel-istanbul check-coverage --branches 100 --functions 100 --lines 100 --statements 100",
        "prepare": "npm run build",
        "prepublishOnly": "npm run securityCheck && npm test --coverage",
        "pretest": "npm run lint",
        "securityCheck": "nsp check",
        "test": "babel-node ./node_modules/.bin/babel-istanbul test _mocha"
    },
    "version": "0.5.0"
}
