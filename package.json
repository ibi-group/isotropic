{
    "author": "Steven Olmsted <steven.olmsted@ibigroup.com>",
    "babel": {
        "plugins": [
            "transform-runtime"
        ],
        "presets": [
            "es2015"
        ],
        "sourceMaps": "inline"
    },
    "bugs": "https://github.com/ibi-group/isotropic/issues",
    "contributors": [
        "Duane Sibilly <duane.sibilly@ibigroup.com>"
    ],
    "dependencies": {
        "isotropic-asap": "~0.2.0",
        "isotropic-create": "~0.2.0",
        "isotropic-error": "~0.2.0",
        "isotropic-for-in": "~0.2.0",
        "isotropic-later": "~0.2.0",
        "isotropic-line-global": "~0.2.0",
        "isotropic-logger-global": "~0.2.0",
        "isotropic-make": "~0.2.0",
        "isotropic-mixin": "~0.2.0",
        "isotropic-property-chainer": "~0.2.0",
        "isotropic-prototype-chain": "~0.2.0",
        "isotropic-pubsub": "~0.2.0",
        "isotropic-timeout": "~0.2.0"
    },
    "description": "A collection of utilities for large JavaScript projects with a consistent API.",
    "devDependencies": {
        "babel-cli": "~6.10.1",
        "babel-istanbul": "~0.11.0",
        "babel-plugin-transform-runtime": "~6.9.0",
        "babel-preset-es2015": "~6.9.0",
        "chai": "~3.5.0",
        "eslint": "~3.0.1",
        "eslint-config-isotropic": "~0.2.0",
        "in-publish": "~2.0.0",
        "mocha": "~2.5.3"
    },
    "eslintConfig": {
        "env": {
            "es6": true
        },
        "extends": "isotropic",
        "parserOptions": {
            "ecmaFeatures": {
                "experimentalObjectRestSpread": true,
                "globalReturn": false,
                "jsx": false
            },
            "ecmaVersion": 6,
            "sourceType": "module"
        },
        "root": true
    },
    "homepage": "https://github.com/ibi-group/isotropic",
    "license": "BSD-3-Clause",
    "name": "isotropic",
    "repository": "ibi-group/isotropic",
    "scripts": {
        "build": "babel js -d .",
        "lint": "eslint js test",
        "posttest": "[ -z \"$npm_config_coverage\" ] || babel-istanbul check-coverage --branches 100 --functions 100 --lines 100 --statements 100",
        "prepublish": "not-in-install && npm test --coverage && npm run build",
        "pretest": "npm run lint",
        "test": "babel-node ./node_modules/.bin/babel-istanbul test _mocha"
    },
    "version": "0.1.0"
}
